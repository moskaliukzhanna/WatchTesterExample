target 'WatchTesterApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WatchTesterApp

  target 'WatchTesterAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WatchTesterAppUITests' do
    # Pods for testing
  end

end

target 'WatchTesterApp WatchKit App' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WatchTesterApp WatchKit App

  target 'WatchTesterApp WatchKit AppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WatchTesterApp WatchKit AppUITests' do

   # pod 'WatchTester', git: 'https://github.com/moskaliukzhanna/WatchTester.git', :branch => 'pod'
   pod 'WatchTester', path: '../WatchTester'

  end

end

target 'WatchTesterApp WatchKit Extension' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

